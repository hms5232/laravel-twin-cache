{
    "name": "hms5232/laravel-twin-cache",
    "description": "Use second cache flexibly in Laravel.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hms5232\\LaravelTwinCache\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hms5232",
            "email": "hms5232@hhming.moe"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hms5232\\LaravelTwinCache\\TwinCacheServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "orchestra/testbench": "^6.0",
        "nunomaduro/collision": "^5.10"
    },
    "scripts": {
        "test": "composer lint ; composer testbench",
        "lint": "./vendor/bin/phpcs",
        "lint:fix": "./vendor/bin/phpcbf",
        "testbench": "./vendor/bin/testbench package:test"
    },
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Hms5232\\LaravelTwinCache\\Tests\\": "tests/"
        }
    }
}
